Algoritmo calculadora
	//Definimos variables
	Definir a, b, suma, resta, multiplicacion Como Entero;
	Definir division Como Real;
	//Asignamos valores a las variables
	a<-27;
	b<-5;
	//Operaciones
	suma<- a + b;
	resta<- a - b;
	multiplicacion<- a * b;
	division<- a / b;
	//Mensaje al usuario
	Escribir "Operaciones";
	Escribir a, "+", b, "=", suma;
	Escribir a, "-", b, "=", resta;
	Escribir a, "*", b, "=", multiplicacion;
	Escribir a, "/", b, "=", division;
	
FinAlgoritmo